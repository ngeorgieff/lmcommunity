        <name>StockPrice</name>
        <displayedas>Stock Price</displayedas>
        <description>Tracks stock prices for symbols defined in the &#34;stocks&#34; property. Data from finnhub.io.</description>
        <collector>batchscript</collector>
        <hasMultiInstances>true</hasMultiInstances>
        <schedule>120</schedule>
        <appliesTo>finnhub.api.key	&#38;&#38; stocks</appliesTo>
        <wildcardauto>true</wildcardauto>
        <wildcardpersist>false</wildcardpersist>
        <wildcardlinuxscript>ad_script</wildcardlinuxscript>
        <wildcardlinuxcmdline>type=&#34;embeded&#34; </wildcardlinuxcmdline>
        <wildcardwinscript>ad_script</wildcardwinscript>
        <wildcardwincmdline>type=&#34;embeded&#34; </wildcardwincmdline>
        <wildcardgroovyscript>import groovy.json.JsonSlurper
stocks = hostProps.get(&#34;stocks&#34;).tokenize(&#34;,| &#34;)
token = hostProps.get(&#34;finnhub.api.key&#34;)
try {
  url = &#34;https://finnhub.io/api/v1/stock/symbol?exchange=US&#38;token=${token}&#34;
  jsondata = url.toURL().getText()
  // println(jsondata)
  data = new JsonSlurper().parseText(jsondata)
  data.each{
     if (stocks.contains(it.symbol)) {
       println(&#34;${it.symbol}##${it.description}&#34;)
     }
  }
} catch (Exception e) {println(e);return 1}
return 0
</wildcardgroovyscript>
